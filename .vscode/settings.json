{
    "java.configuration.updateBuildConfiguration": "automatic",
    "[json]": {
        "editor.formatOnSave": true
    },
    "[java]": {
        "editor.tabSize": 4,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "redhat.java",
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        },
    },
    "java.test.config": [
        {
            "name": "coreTesting",
            "vmArgs": []
        },
        {
            "name": "/chore-manager testing",
            "vmArgs": [
                "--add-opens",
                "main.ui/ui=ALL-UNNAMED",
                "--add-exports",
                "javafx.graphics/com.sun.javafx.application=ALL-UNNAMED"
            ]
        }
    ],
    "java.test.defaultConfig": "/chore-manager testing",
    "java.compile.nullAnalysis.mode": "automatic",
    "java.debug.settings.onBuildFailureProceed": true,
    "java.format.comments.enabled": true,
    "java.cleanup.actionsOnSave": [
        "addOverride",
        "qualifyMembers"
    ],
    "java.saveActions.organizeImports": true,
    "java.completion.importOrder": [
        "#",
        "java",
        "javax",
        "org",
        "com",
        ""
    ],
    "java.checkstyle.configuration": "${workspaceFolder}\\chore-manager\\config\\checkstyle.xml",
    "java.checkstyle.version": "10.12.3",
    "java.format.settings.url": ".vscode/java-formatter.xml",
}